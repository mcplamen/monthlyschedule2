{
    "name": "mcplamen/monthlyschedule",
    "type": "typo3-cms-extension",
    "description": "Simple schedule for appointments in the month",
    "authors": [
        {
            "name": "Plamen Petkov",
            "role": "Developer"
        }
    ],
    "license": "GPL-2.0-or-later",
    "require": {
        "typo3/cms-core": "^11.5"
    },
    "require-dev": {
        "typo3/testing-framework": "^6.9.0"
    },
    "autoload": {
        "psr-4": {
            "Mcplamen\\Monthlyschedule\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mcplamen\\Monthlyschedule\\Tests\\": "Tests"
        }
    },
    "replace": {
        "typo3-ter/monthlyschedule": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "scripts": {
        "post-autoload-dump": [
            "TYPO3\\TestingFramework\\Composer\\ExtensionTestEnvironment::prepare"
        ]
    },
    "extra": {
        "typo3/cms": {
            "web-dir": ".Build/public",
            "extension-key": "monthlyschedule"
        }
    }
}